<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/sipgateSettingsInfo" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:orientation="vertical">
	<TextView android:id="@+id/sipgateSettingsInfoName"
		android:padding="3dip" android:layout_marginTop="5dip"
		android:textSize="20dip" android:textColor="#FFFFFF"
		android:layout_width="fill_parent" android:layout_height="wrap_content" />
	<TextView android:id="@+id/sipgateSettingsInfoInfo"
		android:padding="3dip" android:layout_marginBottom="5dip"
		android:textSize="15dip" android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
</LinearLayout>
