<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<TextView android:layout_height="wrap_content"
		android:textColor="#ffffff" android:text="category"
		android:layout_width="fill_parent" android:id="@+id/CategoryTextView"
		android:background="#808080"></TextView>

	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
		android:paddingLeft="6px" android:paddingRight="6px">

		<ImageView android:id="@+id/IconView" android:src="@drawable/voicemail_unread"
			android:layout_height="32dp" android:layout_width="32dp"
			android:scaleType="fitXY" android:layout_marginRight="6dp"
			android:layout_marginTop="2dp" />

		<LinearLayout android:id="@+id/LinearLayout03"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:orientation="vertical">

			<LinearLayout android:id="@+id/LinearLayout02"
				android:layout_height="wrap_content" android:gravity="left"
				android:layout_width="fill_parent">
				<TextView android:text="element" android:id="@+id/EventTitle"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:ellipsize="end" android:textColor="#ffffff" 
					android:singleLine="true" android:inputType="none" android:gravity="left"
					android:width="200dp" android:layout_gravity="left"></TextView>
				<TextView android:layout_width="wrap_content" android:width="70dp"
					android:layout_height="wrap_content" android:id="@+id/DateTextView"
					android:text="0:00"  android:gravity="right" android:layout_gravity="right"></TextView>
			</LinearLayout>

			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:id="@+id/TranscriptionTextView"
				android:text="transcription"></TextView>
		</LinearLayout>


	</LinearLayout>

</LinearLayout>
