<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/sipgateCallList" android:layout_height="fill_parent"
	android:layout_width="fill_parent" android:orientation="vertical">
	<ListView android:id="@+id/CalllistListView"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:divider="#808080" android:dividerHeight="1px">
	</ListView>
	<TextView android:text="@string/sipgate_empty_calllist"
		android:id="@+id/EmptyCallListTextView" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:gravity="center"
		android:textColor="#FFFFFF"></TextView>
</LinearLayout>